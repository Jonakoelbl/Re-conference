<div class="preamble" id="zen-preamble" role="article">
  <h3 style="color:white"> Tabla de conferencias </h3>
  <table border="1" style="width:500px">
    <tr>
      <th align="center">Nombre</th>
      <th align="center">Descripción</th>
      <th align="center">Fecha</th>
    </tr>
    <% @conferencias.each do | conferencia | %>
        <tr>
          <td align="center">
            <%= conferencia.titulo %>
          </td>
          <td align="center">
            <%= conferencia.descripción%>
          </td>
          <td align="center">
            <%= conferencia.fecha%>
          </td>
        </tr>
    <% end %>
  </table>
  <br>
  <%= link_to "Volver", url(:home, :index), :class => 'btn btn-secondary' %>
</div>
